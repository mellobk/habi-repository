    "@testing-library/user-event": "^12.1.10",
    "cropperjs": "^1.5.12",
    "history": "^4.10.1",
    "modern-normalize": "^1.1.0",
    "phosphor-react": "^1.4.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-cropper": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.33.1",
    "react-modal": "^3.15.1",
    "react-redux": "^7.2.8",
    "react-router-dom": "^5.3.3",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass": "^1.53.0",
    "start": "^5.1.0",
    "styled-components": "^5.3.5",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
